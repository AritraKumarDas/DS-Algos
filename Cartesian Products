// Cartesian product

const arr1 = [1,2];
const arr2 = [4,5,6];

function cartesianProduct(){
    const product=[];

    for(let i=0; i<arr1.length; i++){
        for(let j=0; j<arr2.length; j++){
            product.push([ arr1[i], arr2[j] ])
        }
    } 
    return product;
}

console.log(cartesianProduct(arr1,arr2));

// Big O = O(m x n)
